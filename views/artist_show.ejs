<div class='artist_header'>
	<ul>
		<li><img src="<%= squareAlbumImage(artist) %>" /></li>
		<li>
			<h1><%= artist.name %></h1>
		</li> 
		<li>
			<ul id='artist_videos'>
			</ul>
		</li>
	</ul>
</div>
 
<div class='artist_tracks'>
	
	<ul class='playlist'>
		<% for(var i=0; i < songs.length; i++) { %>
			<% var record = songs[i] %>
			<li>                   
				<a rel=<%= i %> 
					 href='/play/<%= record.song.slug %>' 
					 class='play_song' 
					 data-artisturl='/artists/<%= artist.slug %>' 
					 data-artist='<%= artist.name %>' 
					 data-album='<%= record.album.name %>' 
					 data-song='<%= record.song.name %>'>
						<%= record.album.name %> / <%= record.song.track_number %> / <%= record.song.name %>
				</a>
			</li>
		<% } %>
	</ul>
</div>

<script type="text/javascript" charset="utf-8">
	$(document).ready(function() {
		app.ajax.VideoForArtist('<%= artist.name %>');
	})
</script>
